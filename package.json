{
  "private": true,
  "name": "demo",
  "version": "2.1.0",
  "description": "Security Portal",
  "homepage": "https://rubix410.sketchpixy.com/",
  "engines": {
    "node": "7.8.0"
  },
  "watch": {
    "dev:server": "tmp/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcusferland/secportal.git"
  },
  "scripts": {
    "clean:dist": "rimraf dist",
    "clean:js": "rimraf public/js/main*.js && rimraf public/js/app.js && rimraf public/js/plugins.js && rimraf public/js/*.map",
    "clean:css": "rimraf public/css/main*.css && rimraf public/css/app*.css && rimraf public/css/*.map",
    "clean:fonts": "rimraf public/fonts/*",
    "clean:server": "rimraf server.js && rimraf server.js.map",
    "clean:tmp": "rimraf tmp",
    "clean": "run-s clean:dist clean:tmp clean:server clean:css clean:js clean:fonts",
    "update:schema": "babel-node tools/update-schema.js",
    "hot:client": "cross-env NODE_ENV=development EXTRACT_TEXT_PLUGIN=false webpack-dev-server --config tools/webpack.client.js",
    "hot:server": "cross-env NODE_ENV=development webpack --colors --display-error-details --watch --verbose --config tools/webpack.server.js",
    "dev:server": "cross-env NODE_ENV=development node tmp/bundle.js",
    "dev:server:wait": "just-wait -p \"tmp/bundle.js\" --timeout 120 && npm run start-watcher",
    "start-watcher": "npm-watch",
    "watch-build-server": "run-s clean:tmp hot:server",
    "dev": "rimraf tmp && concurrently --kill-others \"npm run watch-build-server\" \"npm run hot:client\" \"npm run dev:server:wait\"",
    "dev:rtl": "cross-env RTL=true run-s dev",
    "dev:dist": "run-p dev dist:watch",
    "build:server": "cross-env NO_OUTPUT_PATH=true NODE_ENV=production webpack --progress --colors --display-error-details --config tools/webpack.server.js",
    "build:client": "cross-env EXTRACT_TEXT_PLUGIN=true webpack --progress --colors --display-error-details --config tools/webpack.client.js",
    "bundle:server": "run-s clean:tmp clean:server build:server",
    "bundle:client": "run-s clean:css clean:js clean:fonts build:client",
    "prod:server": "cross-env NODE_ENV=production pm2 start server.js",
    "build:prod": "cross-env NODE_ENV=production run-s bundle:client bundle:server",
    "build:prod:rtl": "cross-env NODE_ENV=production RTL=true run-s bundle:client bundle:server",
    "prod": "run-s build:prod prod:server",
    "prod:rtl": "run-s build:prod:rtl prod:server",
    "dist": "cross-env NODE_ENV=production run-s bundle:client && node tools/create-dist.js",
    "dist:watch": "run-s bundle:client && node tools/watch-dist.js",
    "test": "mocha --compilers :./tools/babel-setup.js --recursive --watch ./test/*.js",
    "start": "node server.js"
  },
  "author": "Sketchpixy <support@sketchpixy.com>",
  "devDependencies": {
    "@sketchpixy/just-wait": "^1.0.5",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-relay-plugin-loader": "^0.9.0",
    "bless-webpack-plugin": "^1.0.0",
    "chokidar": "^1.6.1",
    "concurrently": "^2.1.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "deepmerge": "^0.2.10",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "isomorphic-style-loader": "^1.0.0",
    "jsdom": "^9.2.1",
    "json-loader": "^0.5.4",
    "loader-utils": "^0.2.15",
    "mocha": "^3.2.0",
    "ncp": "^2.0.0",
    "node-sass": "^3.8.0",
    "npm-run-all": "^2.2.0",
    "npm-watch": "^0.1.4",
    "null-loader": "^0.1.1",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "rtlcss": "^2.0.5",
    "sass-loader": "^3.2.1",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "uglify-loader": "^1.3.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "@sketchpixy/rubix": "^2.0.0",
    "axios": "^0.15.3",
    "babel-cli": "^6.10.1",
    "body-parser": "^1.17.1",
    "canvas": "^1.6.5",
    "classnames": "^2.2.5",
    "clipboard": "^1.6.1",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "d3-funnel": "^1.0.1",
    "express": "^4.15.2",
    "express-bearer-token": "^2.1.0",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^7.3.0",
    "mime": "^1.3.4",
    "mongoose": "^4.9.5",
    "node-2fa": "^1.1.2",
    "nodemailer": "^3.1.8",
    "passport": "^0.3.2",
    "passport-2fa-totp": "0.0.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pm2": "^2.4.3",
    "pug": "^2.0.0-beta11",
    "qr-image": "^3.2.0",
    "qrcode": "^0.7.1",
    "querystring": "^0.2.0",
    "react": "^15.4.2",
    "react-cookie": "^1.0.5",
    "react-dom": "^15.4.2",
    "react-file-download": "^0.3.2",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-router": "^3.0.0-alpha.3",
    "source-map-support": "^0.4.14",
    "speakeasy": "^2.0.0",
    "step": "^1.0.0",
    "validator": "^7.0.0"
  }
}
